<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>PhotoPostsServlet</servlet-name>
    <servlet-class>servlets.PhotoPostsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhotoPostServlet</servlet-name>
    <servlet-class>servlets.PhotoPostServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PhotoPostsServlet</servlet-name>
    <url-pattern>/photoposts</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhotoPostServlet</servlet-name>
    <url-pattern>/photo-post</url-pattern>
  </servlet-mapping>

</web-app>
